{
         "editor.inlineSuggest.enabled": true,
         "workbench.colorTheme": "Monokai Pro",
         "editor.accessibilitySupport": "on",
         "latex-workshop.latex.tools": [
           {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
             "-synctex=1",
             "-interaction=nonstopmode",
             "-file-line-error",
             "-pdf",
             "-outdir=%OUTDIR%",
             "%DOC%"
            ],
            "env": {}
           },
           {
            "name": "xelatex",
            "command": "xelatex",
            "args": [
             "-synctex=1",
             "-interaction=nonstopmode",
             "-file-line-error",
             "-pdf",
             "-outdir=%OUTDIR%",
             "%DOC%"
            ],
            "env": {}
           },
           {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
             "-synctex=1",
